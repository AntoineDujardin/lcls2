import subprocess

for i in range(3):
    for core_num in [1,2,4,8,10,12,16]:
        comm = 'mpirun -n %i python swmr_write_test.py >> results_write_ztest.txt' % core_num
        print(comm)
        subprocess.call(comm, shell=True)


        comm2 = 'mpirun -n %i python swmr_read_test.py >> results_read_ztest.txt' % core_num
        print(comm2)
        subprocess.call(comm2, shell=True)
